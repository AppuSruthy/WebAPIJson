@model IEnumerable<WebAPIProject.Models.albums>
@{
    ViewBag.Title = "Albums";
}

<h2>GAlbums</h2>
@{
    <table class="table table-striped">
        <tr>
            <th>Id</th>
            <th>UserId</th>
            <th>Title</th>

        </tr>
        @foreach (var item in Model)
        {
    <tr>
        <th>@item.id</th>
        <th>@item.userId</th>
        <th>@item.title</th>


    </tr>
        }
    </table>
}

